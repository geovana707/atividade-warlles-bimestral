#include <stdio.h>

int main() {
    int numero, dezMilhar, milhar, centena, dezena, unidade, resto;

    printf("Informe um numero: \n");
    scanf("%d", &numero);

    if (numero < 10000 || numero > 99999) {
        printf("Digite um numero valido");
        return 0;
    }

    dezMilhar = numero / 10000;
    resto = numero % 10000;

    milhar = resto / 1000;
    resto = resto % 1000;

    centena = resto / 100;
    resto = resto % 100;

    dezena = resto / 10;
    unidade = resto % 10;

    printf("%d   %d   %d   %d   %d \n", dezMilhar, milhar, centena, dezena, unidade);

    return 0;
}
